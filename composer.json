{
	"name": "jalno/lumen",
	"description": "jalno lumen",
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"Jalno\\Lumen\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Jalno\\Lumen\\Tests\\": "tests/"
		}
	},
	"require": {
		"laravel/lumen-framework": "^8.0",
		"jalno/autodiscovery": "^1.0.0"
	},
	"require-dev": {
		"nunomaduro/larastan": "^0.7.6",
		"orchestra/testbench": "^6.18.0",
		"phpunit/phpunit": "^8.5.8|^9.3.3"
	},
	"scripts": {
		"test:types": "./vendor/phpstan/phpstan/phpstan analyse -vvv --debug phpstan.neon src",
		"test:unit": "./vendor/phpunit/phpunit/phpunit",
        "test": [
			"@test:types",
			"@test:unit"
        ]
	},
	"bin": [
		"bin/ConvertToPrimariyPackage"
	]
}
